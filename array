package org.example;
import java.util.Scanner;
public class arraycomapenastamanho {
    public static void main(String[]args){
        String[] Alunos=new String[10];
        Scanner leitor=new Scanner(System.in);
        for (int quant=0;quant<Alunos.length;quant++){
            Alunos[quant]=leitor.next();

        }
        for(int i=0;i<Alunos.length;i++){
            System.out.println("Bem vindo!"+Alunos[i]);
        }
        leitor.close();
    }
}
